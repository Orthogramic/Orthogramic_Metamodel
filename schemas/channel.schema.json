        },
        "geographicCoverage": {
          "type": "object",
          "description": "Locations served",
          "properties": {
            "regions": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "countries": {
              "type": "number"
            },
            "cities": {
              "type": "number"
            }
          }
        },
        "distributionCenters": {
          "type": "array",
          "description": "Key facilities",
          "items": {
            "type": "object",
            "properties": {
              "centerName": {
                "type": "string"
              },
              "location": {
                "type": "string"
              },
              "capacity": {
                "type": "string"
              }
            }
          }
        },
        "transportationModes": {
          "type": "array",
          "description": "Channel methods",
          "items": {
            "type": "object",
            "properties": {
              "modeName": {
                "type": "string"
              },
              "percentage": {
                "type": "number"
              },
              "costPerUnit": {
                "type": "string"
              }
            }
          }
        },
        "networkCapacity": {
          "type": "object",
          "description": "Throughput capability",
          "properties": {
            "totalCapacity": {
              "type": "string"
            },
            "utilizationRate": {
              "type": "string"
            },
            "peakCapacity": {
              "type": "string"
            }
          }
        },
        "serviceLevel": {
          "type": "array",
          "description": "Performance standards",
          "items": {
            "type": "object",
            "properties": {
              "metricName": {
                "type": "string"
              },
              "target": {
                "type": "string"
              },
              "actual": {
                "type": "string"
              }
            }
          }
        },
        "leadTime": {
          "type": "object",
          "description": "Fulfillment time",
          "properties": {
            "averageLeadTime": {
              "type": "string"
            },
            "variability": {
              "type": "string"
            }
          }
        },
        "networkCost": {
          "type": "object",
          "description": "Cost structure",
          "properties": {
            "totalAnnualCost": {
              "type": "string"
            },
            "fixedCosts": {
              "type": "number"
            },
            "variableCosts": {
              "type": "number"
            }
          }
        },
        "networkFlexibility": {
          "type": "object",
          "description": "Adaptability",
          "properties": {
            "scalingCapability": {
              "type": "string"
            }
          }
        },
        "networkResilience": {
          "type": "object",
          "description": "Disruption resistance",
          "properties": {
            "riskAssessment": {
              "type": "string"
            },
            "backupCapability": {
              "type": "string"
            }
          }
        },
        "technologyEnablers": {
          "type": "array",
          "description": "Supporting technologies",
          "items": {
            "type": "object",
            "properties": {
              "technologyName": {
                "type": "string"
              },
              "maturityLevel": {
                "type": "string"
              }
            }
          }
        },
        "networkOptimizationStrategy": {
          "type": "object",
          "description": "Improvement approach",
          "properties": {
            "focusAreas": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "expectedOutcomes": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          }
        }
      }
    },
    "channelIntegration": {
      "type": "object",
      "description": "How different channels connect and work together",
      "properties": {
        "integrationTitle": {
          "type": "string",
          "description": "Name of channel integration"
        },
        "integrationDescription": {
          "type": "string",
          "description": "Detailed description"
        },
        "channelsIntegrated": {
          "type": "array",
          "description": "Channels connected",
          "items": {
            "type": "string"
          }
        },
        "integrationPurpose": {
          "type": "array",
          "description": "Integration goals",
          "items": {
            "type": "string"
          }
        },
        "integrationArchitecture": {
          "type": "object",
          "description": "Technical approach",
          "properties": {
            "architectureType": {
              "type": "string"
            },
            "dataIntegrationApproach": {
              "type": "string"
            }
          }
        },
        "customerExperienceImpact": {
